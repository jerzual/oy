{
    "compact": false,
    "presets": [
      ["@babel/preset-env", {
        "loose": true, 
        "targets": {
          "esmodules": false
        }}]
    ],
    "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-object-rest-spread",
          "@babel/plugin-syntax-jsx",
      ["babel-plugin-inferno", {"imports": true}]
    ]
  }