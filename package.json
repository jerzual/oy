{
  "name": "@jerzual/uoy",
  "version": "1.0.0",
  "description": "a rogue platformer",
  "main": "www/server.js",
  "scripts": {
    "compile:client": "tsc",
    "compile:server": "tsc -p tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack -p",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack.server.config.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --runInBand --coverage",
    "watch": "webpack-dev-server --progress --hot --colors --mode development",
    "watch:prod": "cross-env NODE_ENV=production webpack-dev-server --progress --colors --content-base dist/www --mode production",
    "watch:server": "nodemon --watch 'app/**/*.ts' --ignore 'app/**/*.spec.ts' --exec ts-node app/server.ts",
    "lint": "tslint src/*.ts{,x} src/**/*.ts{,x}",
    "lint:force": "tslint --force --project .",
    "start": "node server.js",
    "typedoc": "typedoc --out ./reports/documentation/ --verbose --exclude \"**/*.{spec.ts,scss}\" --includeDeclarations typings/ ./app/main.ts ",
    "webdriver-start": "webdriver-manager start &",
    "webdriver-update": "webdriver-manager update",
    "protractor": "protractor",
    "stats": "webpack --json > stats.json",
    "e2e": "tsc protractor.conf.ts --outDir . --module CommonJS && protractor",
    "e2e:ci": "tsc protractor.conf.ts --outDir . && protractor --capabilities.chromeOptions.args=--headless --capabilities.chromeOptions.args=--no-sandbox",
    "prf": "artillery run prf/scenario.yml"
  },
  "keywords": [
    "platformer",
    "rogue-like",
    "pixi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jerzual/oy.git"
  },
  "bugs": {
    "url": "https://github.com/jerzual/oy/issues"
  },
  "author": "jibhaine",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.1",
    "chance": "^1.1.5",
    "express": "^4.17.1",
    "inferno": "^7.4.2",
    "inferno-router": "^7.4.2",
    "inferno-server": "^7.4.2",
    "jsdom": "^16.2.2",
    "matter-js": "^0.14.2",
    "pixi.js": "^5.2.4",
    "pug": "^2.0.4",
    "reactive-state": "^3.5.0",
    "reflect-metadata": "^0.1.13",
    "rot-js": "^2.1.1",
    "rxjs": "^6.5.5",
    "schemapack": "^1.4.2",
    "seedrandom": "^3.0.5",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.0.0",
    "webrtc-adapter": "^7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@types/artillery": "^1.6.2",
    "@types/chance": "^1.1.0",
    "@types/cucumber": "^6.0.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.2",
    "@types/matter-js": "^0.14.3",
    "@types/node": "^14.0.1",
    "@types/pug": "^2.0.4",
    "@types/seedrandom": "^2.4.28",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/socket.io": "^2.1.5",
    "@types/socket.io-client": "^1.4.33",
    "@types/start-server-webpack-plugin": "^2.2.0",
    "@types/supertest": "^2.0.9",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.13",
    "@types/webpack-dev-server": "^3.10.1",
    "artillery": "^1.6.0",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-inferno": "^6.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "injection-js": "^2.3.0",
    "jest": "^26.0.1",
    "jest-marbles": "^2.5.1",
    "json-loader": "^0.5.7",
    "log4js": "^6.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "transform-loader": "^0.2.4",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|(j|t)sx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
